---
interface Props {
	title?: string;
}

const { title = "Estimulación Temprana Burbujitas - Próximamente" } = Astro.props;
---

<!doctype html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Estimulación Temprana Burbujitas - Centro especializado en el desarrollo integral infantil" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>